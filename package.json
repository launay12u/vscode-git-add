{
    "name": "git-add",
    "displayName": "git-add",
    "description": "make a git add on current opened file",
    "version": "1",
    "publisher": "launay12u",
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.gitAdd"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.gitAdd",
                "title": "git-add : add file to git repo"
            }
        ],
        "keybindings": [
            {
            "command": "extension.gitAdd",
            "key": "ctrl+shift+a",
            "mac": "cmd+shift+a"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}
